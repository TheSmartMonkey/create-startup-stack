{
  "name": "queue-service",
  "version": "1.0.0",
  "scripts": {
    "lint": "eslint src --ext .ts",
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "deploy": "sls deploy --verbose",
    "deploy:prod": "AWS_STAGE=prodv1 ENV_TYPE=prod AWS_PROFILE=prod npm run deploy",
    "undeploy": "sls remove --verbose",
    "test": "NO_LOGS=true npm run unit && npm run integration",
    "unit": "NO_LOGS=true jest --group=unit",
    "integration": "NO_LOGS=true jest --group=integration --maxWorkers=1"
  },
  "engines": {
    "npm": "9.5.1",
    "node": "18.16.0"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.282.0",
    "async": "^3.2.4",
    "aws-sdk": "2.1311.0",
    "dotenv": "^16.0.2",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.2.0",
    "mongodb": "^5.1.0",
    "mongoose": "^6.10.0",
    "pino": "^8.6.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@serverless/typescript": "^3.21.0",
    "@types/async": "^3.2.18",
    "@types/aws-lambda": "^8.10.106",
    "@types/dotenv": "^8.2.0",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.7.20",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "axios": "^1.3.4",
    "esbuild": "^0.15.9",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.24.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
    "jest": "^29.3.1",
    "jest-runner-groups": "^2.2.0",
    "json-schema-to-ts": "^2.6.2",
    "prettier": "^2.8.4",
    "serverless": "^3.22.0",
    "serverless-deployment-bucket": "^1.6.0",
    "serverless-esbuild": "^1.33.0",
    "serverless-plugin-typescript": "^2.1.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.5"
  }
}
